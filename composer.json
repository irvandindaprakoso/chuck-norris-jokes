{
    "name": "irvandindaprakoso/chuck-norris-jokes",
    "description": "Create random Chuck Norris Jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Irvan Dinda Prakoso",
            "email": "irvandindaprakoso@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Irvandindaprakoso\\ChuckNorrisJokes\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Irvandindaprakoso\\ChuckNorrisJokes\\Tests\\" : "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Irvandindaprakoso\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "Irvandindaprakoso\\ChuckNorrisJokes\\Facades\\ChuckNorris"
            }
        }
    }
}
